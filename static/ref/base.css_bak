}
.sidebar {
	position: fixed;
	bottom: 0;
	left: 45;
	z-index: 1000;
	display: block;
	width: 100%;
	overflow-x: hidden;
	overflow-y: auto;
	-webkit-box-shadow: 0 0 7px 0 rgba(119, 119, 119, 0.2);
	box-shadow: 0 0 7px 0 rgba(119, 119, 119, 0.2);
	background: -webkit-linear-gradient(left, #e9e9e9, #fafafa);
	border-right: 1px solid #d9d9d9;
	zoom: 1 box-shadow: 0px 0px 7px 0px rgba(119, 119, 119, 0.2);
	-ms-box-shadow: 0 0 7px 0 rgba(119, 119, 119, 0.2);
	background: -ms-linear-gradient(left, rgb(233, 233, 233),
		rgb(250, 250, 250));
}

.main-nav {
	border-bottom: 1px solid #f9f9f9;
}

.main-nav-in {
	cursor: pointer;
	width: 44px;
	height: 30px;
	padding: 12px 0 0 12px;
