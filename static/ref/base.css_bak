#maincontent{
	box-shadow: 1px 0 0px 0px rgba(0,0,0,.3);
	background-color: transparent;
}

.nav-tabs>li {
	white-space: nowrap;
}

.nav-tabs>li>div {
	border-radius: 0px;
	text-align: center;
	cursor: pointer;
	white-space: nowrap;
	overflow: hidden;
	min-width: 100px;
}

.nav-tabs>li.active>div {
	border-top-width: 3px;
	border-top-style: solid;
	border-top-color: #37a8e0;
	background-color: #fff;
	font-weight: bold;
}

.nav-tabs>li>div>span {
	display: inline-block;
	padding: 10px;
}

.nav-tabs>li.active>div>span {
	display: inline-block;
	padding: 9px 10px 8px 10px;
}

.nav-tabs>li>div>button {
	margin-top: 10px;
	margin-right: 5px;
}

.nav-tabs>li.active>div>button {
	margin-top: 7px;
}

.main>.tab-content>.tab-pane>.row {
	margin: 0px;
}

.mainside {
	border-bottom: 1px solid #EEEEEE;
	background: #ffffff;
}

@media screen and (min-width:768px) {
	.mainside {
		position: fixed;
		top: 98px;
		bottom: 0px;
		display: block;
		overflow-x: hidden;
		overflow-y: auto;
		/*background-color:#F9F9F9;*/
		border-right: 1px solid #EEEEEE;
	}
}

.mainside>.list-group {
	margin-left: -16px;
	margin-right: -16px;
}

.mainside>.list-group>.list-group-item {
	border-radius: 0px;
}

.mainside>.nav {
	margin-left: -16px;
	margin-right: -16px;
	padding-top: 10px;
	padding-bottom: 10px;
}

.mainside>.navBottom {
	border-bottom: 1px solid #EEEEEE;
	text-align: center;
	padding: 15px;
	margin: 0 -15px 0 -15px;
}
.mainside>.nav>li{
	margin-left: 15px;
}
.mainside>.nav>li>a {
	color: #000;
	border-radius: 0px;
	margin-left: 15px;
}

.mainside>.nav>li>a>i {
	padding-right: 15px;
	padding-left: 10px;
	font-size: 16px;
}

.mainside>.nav>li.active>a>i {
	color: #FFF;
}

