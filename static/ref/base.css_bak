}

.iwf-grid-split {
	border-bottom-width: 1px;
	border-bottom-style: solid;
	border-bottom-color: #999999;
	font-weight: bold;
	line-height: 30px;
	cursor: default;
	background-color: #F3F7FA;
	white-space: nowrap;
	overflow: hidden;
}

